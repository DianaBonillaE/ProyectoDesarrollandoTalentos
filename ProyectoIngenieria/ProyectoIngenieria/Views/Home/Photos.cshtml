@{
    ViewBag.Title = "Photos";
}

@Html.Partial("UserMenu")

<!-- Header -->
<header class="masthead">
    <div class="container">
        <div class="intro-text">

        </div>
    </div>
</header>

<!-- Portfolio Grid -->
<section class="bg-light page-section" id="portfolio2">

    <div class="row">
        <div class="col-lg-12 text-center">
            <h2 class="section-heading text-uppercase">Fotografías</h2>
            <br />
        </div>
    </div>
    <div id="demo" class="carousel slide" data-ride="carousel">

        <!-- Indicators -->
        <ul class="carousel-indicators">
            <li data-target="#demo" data-slide-to="0" class="active"></li>
            <li data-target="#demo" data-slide-to="1"></li>
            <li data-target="#demo" data-slide-to="2"></li>
        </ul>

        <!-- The slideshow -->
        <div class="carousel-inner">
            <div class="carousel-item active">
                <div class="row">

                    @foreach (var item in ViewBag.photo)
                    {
                        <div class="col-md-4 col-sm-6 portfolio-item">
                           
                                
                                <img class="img-fluid" src="~/Static/@item.image" alt="">
                           
                            <div class="portfolio-caption">
                                <h4>@item.name</h4>

                            </div>



                        </div>

                    }




                </div>
            </div>

        </div>


    </div>

</section>


